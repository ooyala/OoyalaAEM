<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured">
        <sidepanels jcr:primaryType="nt:unstructured">
            <edit jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <tabs jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <assetsTab jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <filterPanel jcr:primaryType="nt:unstructured">
                                        <items jcr:primaryType="nt:unstructured">
                                            <search jcr:primaryType="nt:unstructured">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <searchpanel jcr:primaryType="nt:unstructured">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <ooyalapath
                                                                granite:class="assetfilter path"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/coral/common/pathfields7"
                                                                emptyText="Enter path"
                                                                name="assetfilter_ooyala_path">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    filterType="Ooyala"/>
                                                            </ooyalapath>
                                                            <fulltext
                                                                granite:id="assetsearch"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="ooyala/components/content/suggestpredicate"
                                                                emptyText="Filter"
                                                                name="fulltext">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"/>
                                                            </fulltext>
                                                        </items>
                                                    </searchpanel>
                                                    <searchpanel2 jcr:primaryType="nt:unstructured">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <ooyalafilters
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/common/admin/includefacets"
                                                                path="cq/search/facets/ooyala"/>
                                                        </items>
                                                    </searchpanel2>
                                                </items>
                                            </search>
                                        </items>
                                    </filterPanel>
                                </items>
                            </assetsTab>
                        </items>
                    </tabs>
                </items>
            </edit>
        </sidepanels>
    </jcr:content>
</jcr:root>
